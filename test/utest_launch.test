<?xml version="1.0"?>
<launch>
    <node pkg="azure_kinect_ros_driver" type="k4a_exposure_calibration_node" name="k4a_exposure_calibration"/>
    <test test-name="unit_test_node" pkg="azure_kinect_ros_driver" type="utest_node"/>
    <test name="publishtest" test-name="publishtest" pkg="rostest" type="publishtest">
        <rosparam>
            topics:
                - name: "/points2"
                timeout: 10
                negative: False
                - name: "/rgb/raw/image"
                timeout: 10
                negative: False
        </rosparam>
    </test>
</launch>